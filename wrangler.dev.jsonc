/**
 * Development configuration for Wrangler
 * This overrides settings from wrangler.jsonc for local development
 */
{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "google-tag-manager-mcp-server-dev",
	"main": "src/index.ts",
	"compatibility_date": "2025-03-10",
	"compatibility_flags": ["nodejs_compat"],
	// No custom domain for local development
	"routes": [],
	"migrations": [
		{
			"new_sqlite_classes": ["GoogleTagManagerMCPServer"],
			"tag": "v1"
		}
	],
	"durable_objects": {
		"bindings": [
			{
				"class_name": "GoogleTagManagerMCPServer",
				"name": "MCP_OBJECT"
			}
		]
	},
	"kv_namespaces": [
		{
			"binding": "OAUTH_KV",
			"id": "local-oauth-kv"
		}
	],
	"observability": {
		"enabled": true
	},
	"dev": {
		"port": 8788,
		"local_protocol": "http"
	}
}
